<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder" 
             xmlns:g="urn:import:com.google.gwt.user.client.ui" 
             xmlns:c="urn:import:com.google.gwt.user.cellview.client"
             xmlns:meteo-widget="urn:import:es.uned.grc.pfc.meteo.client.view.widget"
             ui:generateFormat='com.google.gwt.i18n.rebind.format.PropertiesFormat'
             ui:generateKeys="com.google.gwt.i18n.rebind.keygen.MD5KeyGenerator"
             ui:generateLocales="default">

  <g:HorizontalPanel ui:field="containerPanel">
    <meteo-widget:ValueTextBox ui:field="valueTextBox"/>
    <g:VerticalPanel>
      <g:PushButton ui:field="upPushButton"/>
      <g:PushButton ui:field="downPushButton"/>
    </g:VerticalPanel>
  </g:HorizontalPanel>
</ui:UiBinder>
