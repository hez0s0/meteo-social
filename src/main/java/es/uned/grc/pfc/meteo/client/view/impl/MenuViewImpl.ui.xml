<ui:UiBinder
  xmlns:ui='urn:ui:com.google.gwt.uibinder' 
  xmlns:g='urn:import:com.google.gwt.user.client.ui'
  xmlns:c="urn:import:com.google.gwt.user.cellview.client"
  ui:generateFormat='com.google.gwt.i18n.rebind.format.PropertiesFormat'
  ui:generateKeys="com.google.gwt.i18n.rebind.keygen.MD5KeyGenerator"
  ui:generateLocales="default">
  
  <g:HTMLPanel>
    <g:MenuBar ui:field="menuBar" styleName="bar" autoOpen="true" animationEnabled="true">
      <g:MenuItem ui:field="refreshMenuItem" styleName="barItem" width="90%">
         <img src="images/icons/arrow_refresh.png" alt="Actualizar vista actual"/>
         <ui:msg meaning="the menu item" description="Refresh menu item">Actualizar vista actual</ui:msg>
      </g:MenuItem>
    </g:MenuBar>
  </g:HTMLPanel>
</ui:UiBinder>
