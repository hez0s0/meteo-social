<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder" 
             xmlns:g="urn:import:com.google.gwt.user.client.ui" 
             xmlns:c="urn:import:com.google.gwt.user.cellview.client"
             xmlns:meteo-suggest="urn:import:es.uned.grc.pfc.meteo.client.view.widget.suggest"
             ui:generateFormat='com.google.gwt.i18n.rebind.format.PropertiesFormat'
             ui:generateKeys="com.google.gwt.i18n.rebind.keygen.MD5KeyGenerator"
             ui:generateLocales="default">
     
  <g:FocusPanel ui:field="rootFocusPanel">        
    <g:FlowPanel ui:field="rootFlowPanel">
       <meteo-suggest:BulletList ui:field="bulletList">
         <meteo-suggest:ListItem ui:field="defaultListItem"/>
       </meteo-suggest:BulletList>
    </g:FlowPanel>
  </g:FocusPanel>
</ui:UiBinder>
